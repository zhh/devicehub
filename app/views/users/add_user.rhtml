<%= error_messages_for 'user' %>
<h3><%= _("Create A New User") %></h3>
<% form_for :user, :url => { :action => :add_user }, :html => { :class => "new_project" } do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p class="btns right">
    <%= submit_tag _("Save"), :class => "button" %>
  </p>
<% end %>
