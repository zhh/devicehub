<%= error_messages_for 'area' %>
<div class="group">
  <h3><%= _("Move A Area") %></h3>
  <% form_for :area, :url => { :action => :move }, :html => { :class => "new_project" } do |f| %>
    <%= render :partial => 'form_move', :locals => { :f => f } %>
    <p class="btns right">
      <%= submit_tag _("Save"), :class => "button", :disable_with => _("Saving") %>
    </p>
  <% end %>
</div>
