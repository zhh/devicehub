<%= error_messages_for 'area' %>
<div class="group">
  <h3><%= _("Modify A New Area") %></h3>
  <% form_for :area, :url => { :action => :edit }, :html => { :class => "new_project" } do |f| %>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <p class="btns right">
      <%= submit_tag _("Save"), :class => "button" %>
    </p>
  <% end %>
</div>
