<%= error_messages_for 'project' %>
<h3><%= _("Create A New Project") %></h3>
<% form_for :project, :url => { :action => :edit }, :html => { :class => "new_project" } do |f| %>
  <%= render :partial => 'form', :locals => { :f => f } %>
  <p class="btns right">
    <%= submit_tag _("Save"), :class => "button" %>
  </p>
<% end %>