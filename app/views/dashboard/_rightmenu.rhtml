<h3><%= _("Project|List") %></h3>
<ul class="stacked wbadges" id="projects">
    <% if @projects.empty? %>
      <li style="">
        <%= link_to _("Create A New Project"), :controller => 'projects', :action => 'new' %>
      </li>
    <% else %>
      <% @projects.each do |project| %>
        <ul id="projects" class="stacked wbadges">          
          <li class="proj"><%= link_to "<span class='badge'>0</span>" + project.name, :action => '' %></li>
        </ul>
      <% end %>
    <% end %>
</ul>
